{
  "name": "phonebook-backend",
  "version": "0.0.1",
  "description": "Backend server for phonebook app",
  "main": "index.js",
  "scripts": {
    "build:ui": "rm -rf build && cd ../fso-coursework/part2/phonebook && npm run build --prod && cp -r build ../../../fso-coursework-part3",
